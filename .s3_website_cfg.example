---
site: '.'
s3_bucket: 'example.com'
cloudfront_distribution_id: 'XXXXXXXXXXX'
s3_endpoint: 'ap-southeast-2'

cache_rules:
  - match: "/assets/*"
    maxage: 30 days

  - match: "/css/*"
    maxage: 30 days
    gzip: false

  - match: "*"
    exclude: "/assets"
    maxage: 1 hour

maxage: '1 hour'
gzip: true
